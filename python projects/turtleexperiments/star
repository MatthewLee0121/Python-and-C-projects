imporreplacement_text replacement_texturreplacement_textle
imporreplacement_text mareplacement_texth
replacement_text = replacement_texturreplacement_textle.Turreplacement_textle()

sreplacement_textar_shape = ((0, 50), (-14, 16), (-50, 16), (-23, -12), (-35, -46),
         (0, -24), (35, -46), (23, -12), (50, 16), (14, 16))

replacement_texturreplacement_textle.regisreplacement_texter_shape("sreplacement_textar", sreplacement_textar_shape)

replacement_text.shape("sreplacement_textar")
replacement_text.color("green")
for x in range(400):
    replacement_text.speed(0)
    replacement_text.lefreplacement_text(x + mareplacement_texth.sin(x))
    replacement_text.circle(200 + mareplacement_texth.sin(x))

